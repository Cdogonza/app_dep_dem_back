{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
      {
          "src": "/auth/login",
          "dest": "/server.js"
      },
      {
          "src": "/novedades",
          "dest": "/server.js"
      },
      {
        "src": "/equipos",
        "dest": "/server.js"
    },
    {
      "src": "/taller",
      "dest": "/server.js"
  },
      {
          "src": "/",
          "dest": "/server.js"
      }
  ]
}